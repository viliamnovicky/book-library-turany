{
  "name": "library_turany",
  "version": "1.0.0",
  "description": "Collection of books from library in Turany nad Ondavou",
  "main": "public/js/bundle.js",
  "scripts": {
    "start": "nodemon server.js",
    "start:prod": "NODE_ENV=production nodemon server.js",
    "debug": "ndb server.js",
    "watch:js": "parcel watch ./public/js/index.js --out-dir ./public/js --out-file bundle.js --public-url .",
    "build:js": "parcel build ./public/js/index.js --out-dir ./public/js --out-file bundle.js --public-url .",
    "watch:sass": "sass scss/style.scss public/css/style.css -w",
    "compile:sass": "sass scss/style.scss public/css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' public/css/style.comp.css -o css/style.prefix.css",
    "compress:css": "sass public/css/style.prefix.css public/css/style.css --style compressed",
    "build:css": "npm-run-all compile:sass prefix:css compress:css"
  },
  "author": "Viliam Novicky",
  "license": "ISC",
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "@parcel/config-default": "^2.8.3",
    "@parcel/core": "^2.8.3",
    "@parcel/diagnostic": "^2.8.3",
    "@parcel/events": "^2.8.3",
    "@parcel/fs": "^2.8.3",
    "@parcel/logger": "^2.8.3",
    "@parcel/package-manager": "^2.8.3",
    "@parcel/reporter-cli": "^2.8.3",
    "@parcel/reporter-dev-server": "^2.8.3",
    "@parcel/utils": "^2.8.3",
    "axios": "^1.3.4",
    "bcrypt": "^5.1.0",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "crypto": "^1.0.1",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^6.7.0",
    "helmet": "^6.0.1",
    "helpers": "^0.0.6",
    "hpp": "^0.2.3",
    "http2": "^3.3.7",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^6.9.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.1",
    "nodemon": "^2.0.20",
    "pug": "^3.0.2",
    "sharp": "^0.31.3",
    "slugify": "^1.6.5",
    "validator": "^13.9.0",
    "xss-clean": "^0.1.1"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.5",
    "prettier": "^2.8.4",
    "sass": "^1.58.3"
  }
}
